{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Antecedent</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Allergies</th>
                <td>{{ entity.allergies }}</td>
            </tr>
            <tr>
                <th>Autres</th>
                <td>{{ entity.autres }}</td>
            </tr>
            <tr>
                <th>Traitement</th>
                <td>{{ entity.traitement }}</td>
            </tr>
            <tr>
                <th>Chirurgicaux</th>
                <td>{{ entity.chirurgicaux }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('antecedent') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('antecedent_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
